#include<stdio.h>
int main()
{
	int a[]={15,12,3,5,6};
	int i,target = 6,count=0,temp;
	int n=sizeof(a)/sizeof(a[0]);
	for(i=0;i<n;i++)
	{
		if(a[i]==target)
		{
			count=1;
			temp=i;
		}
	}
	if(count==1)
	{
		printf("The target found at position %d ",temp);
	}	
	else
	{
		printf("The target is not found ");
	}
}
